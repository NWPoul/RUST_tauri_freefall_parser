{
  "build": {
    "beforeDevCommand": "pnpm dev-front",
    "beforeBuildCommand": "vite build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "FREEFALL_PARSER",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "all": true,
        "setFullscreen": true
      },
      "fs": {
        "scope": ["$RESOURCE/*"]
      }
    },
    "bundle": {
      "active": false,
      "targets": "all",
      "identifier": "freefall-parser",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [
        // "D:/DEV/RUST/FS_APP/src-tauri/tauri.conf.json5",
        // "D:/DEV/RUST/FS_APP/src/serv_window/index.html"
      ],
      "windows": {
        "webviewInstallMode": {
          //   "type": "offlineInstaller"
          "type": "embedBootstrapper"
        }
      }
    },
    "security": {
      "csp": null
    },
    "windows": [
        {
            "label": "MAIN",
            "title": "freefall parser",
            "fullscreen": false,
            "resizable": false,
            "width": 300,
            "height": 600,
            "alwaysOnTop": true
        }
    ]
  }
}